{
  "name": "vite-vue-ts-seed",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "dev:mock": "vite --open --mode mock",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx --max-warnings 0",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
