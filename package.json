    "axios": "^1.5.0",
    "dayjs": "^1.11.9",
    "element-plus": "^2.3.12",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "screenfull": "^6.0.2",
    "vue": "^3.3.4",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/lodash": "^4.14.198",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.5.9",
    "@types/nprogress": "^0.2.0",
    "@types/vue-router": "^2.0.0",
